        <script><![CDATA[function transform(input) {
    var markers = [];
    var hideMap = (gs.getProperty("sn_fsm_disp_wrkspc.dispatcher_workspace.hide_map") === "true");
    if (!hideMap) {
        //Get Wind Turbines
        var mapUtil = new x_snc_wind_turbine.WTWorkSpaceUtil();
        markers = mapUtil.getAgentWindTurbines();

    }
    return {
        "markers": markers
    };

}]]></script>
